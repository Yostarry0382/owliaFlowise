# Flowise Configuration
# Flowise APIのURL（デフォルト: http://localhost:3000）
FLOWISE_API_URL=http://localhost:3000

# Flowise API Key（必須: FlowiseダッシュボードのAPI Keysから取得）
FLOWISE_API_KEY=

# デフォルトのChatflow ID（オプション）
FLOWISE_DEFAULT_CHATFLOW_ID=

# Flowise Embed Configuration（フロントエンド用）
# チャットUIで使用するFlowiseのURL
NEXT_PUBLIC_FLOWISE_API_URL=http://localhost:3000

# チャットUIで使用するデフォルトのChatflow ID
# 設定すると、メインページにチャットバブルが表示されます
NEXT_PUBLIC_FLOWISE_DEFAULT_CHATFLOW_ID=

# LLM Configuration（Flowise経由で使用される場合）
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# Database Configuration（統計データ用、オプション）
DATABASE_URL=